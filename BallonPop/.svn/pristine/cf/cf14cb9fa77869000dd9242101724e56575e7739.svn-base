#include "MyObjectList.h"
#include "MyObject.h"
#include <cstddef>

void MyObjectList::MoveObjects()
{
   for (int i = 0; i < objects.size(); i++)
      objects[i]->Move();

   for (int i = 0; i < objects.size(); i++)
      objects[i]->Show();
}

void MyObjectList::Add(MyObject* name)
{
   objects.push_back(name);
}

